{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}\\dist\\app.js",
      "outFiles": ["${workspaceFolder}/**/*.js"],
      "preLaunchTask": "CompileTypeScript",
      "internalConsoleOptions": "openOnSessionStart"
    }
  ]
}
